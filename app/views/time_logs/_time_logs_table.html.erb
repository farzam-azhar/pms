  <div class="card unset-height">
    <div class="card_container custom-padding">
      <div class="row mb-1">
        <div id="time-logs-errors"></div>
        <table class="table table-striped table-hover display" id="time-logs-table">
          <thead>
            <tr>
              <th>Username</th>
              <th>Project Title</th>
              <th>Date</th>
              <th>Start Time</th>
              <th>End Time</th>
              <% if is_not_admin?(current_user)  %>
                <th>Options</th>
              <% end %>
            </tr>
          </thead>
          <tbody id="time-logs">
            <%= render @time_logs %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
