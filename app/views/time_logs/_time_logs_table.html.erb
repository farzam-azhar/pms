  <div class="card unset-height">
    <div class="card_container custom-padding">
      <div class="row mb-1">
        <div id="time-logs-index-error"></div>
        <table class="table table-striped table-hover display" id="time-logs-table">
          <thead>
            <tr>
              <th>Username</th>
              <th>Project Title</th>
              <th>Date</th>
              <th>Start Time</th>
              <th>End Time</th>
              <th>Options</th>
            </tr>
          </thead>
          <tbody id="time-logs">
            <%= render @time_logs %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
